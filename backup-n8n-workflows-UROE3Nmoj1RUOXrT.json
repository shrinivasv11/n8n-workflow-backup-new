ove JSON to binary3').all();\n// Items coming into this node are from: Get existing file content [GITHUB]1\nconst githubItems = $input.all();\n\nreturn githubItems.map((item, index) => {\n  // Get the file name from GitHub response\n  const githubFileName = item.json?.name;\n  \n  // Find matching new workflow by comparing file names\n  const newItem = newItems.find(i => {\n    const newFileName = i.binary?.data?.fileName;\n    return newFileName === githubFileName;\n  });\n  \n  let needsUpdate = false;\n  let newStr = '';\n  let oldStr = '';\n  \n  try {\n    if (!newItem) {\n      // No matching new workflow found\n      needsUpdate = false;\n      console.log(`No match found for: ${githubFileName}`);\n    } else {\n      // Extract new workflow content from n8n (JSON format)\n      let newContent = '';\n      if (newItem.json?.nodes) {\n        newContent = JSON.stringify(newItem.json);\n      } else if (newItem.binary?.data?.data) {\n        // If it's in binary, decode it\n        newContent = Buffer.from(newItem.binary.data.data, 'base64').toString('utf8');\n      }\n      \n      // Extract old file content from GitHub (binary format)\n      let oldContent = '';\n      if (item.binary?.data?.data) {\n        // GitHub returns content in binary format\n        oldContent = Buffer.from(item.binary.data.data, 'base64').toString('utf8');\n      } else if (item.json?.data) {\n        // Fallback if it's in json.data\n        oldContent = Buffer.from(item.json.data, 'base64').toString('utf8');\n      }\n      \n      if (newContent && oldContent) {\n        try {\n          // Parse both as JSON\n          const newJson = JSON.parse(newContent);\n          const oldJson = JSON.parse(oldContent);\n          \n          // Stringify for comparison (handles formatting differences)\n          newStr = JSON.stringify(newJson);\n          oldStr = JSON.stringify(oldJson);\n          \n          needsUpdate = (newStr !== oldStr);\n          console.log(`Comparing ${githubFileName}: Match=${newStr === oldStr}`);\n        } catch (parseError) {\n          // If JSON parse fails, compare as strings\n          newStr = newContent;\n          oldStr = oldContent;\n          needsUpdate = (newStr !== oldStr);\n          console.log(`String comparison for ${githubFileName}: Match=${newStr === oldStr}`);\n        }\n      } else if (newContent && !oldContent) {\n        newStr = newContent;\n        oldStr = '';\n        needsUpdate = true;\n      } else {\n        console.log(`Missing content for ${githubFileName}: new=${!!newContent}, old=${!!oldContent}`);\n        needsUpdate = false;\n      }\n    }\n  } catch (error) {\n    console.log(`Error comparing file ${githubFileName}: ${error.message}`);\n    needsUpdate = false;\n  }\n  \n  // Preserve binary data from new item for upload\n  if (newItem?.binary) {\n    item.binary = newItem.binary;\n  }\n  \n  // Set the needsUpdate flag and debug info\n  item.json.needsUpdate = needsUpdate;\n  item.json.fileName = githubFileName;\n  item.json.matchFound = !!newItem;\n  item.json.newStrLength = newStr.length;\n  item.json.oldStrLength = oldStr.length;\n  item.json.stringsMatch = (newStr === oldStr);\n  // Only include first 500 chars to avoid huge outputs\n  item.json.newStrPreview = newItems\n  item.json.oldStrPreview = oldStr\n  \n  return item;\n});"},"type":"n8n-nodes-base.code","typeVersion":2,"position":[-640,2656],"id":"fe9a08cb-096a-4d7e-a373-bc3ef031d9ee","name":"Code in JavaScript"}],"connections":{"Execute workflow":{"main":[[{"node":"Set date2","type":"main","index":0}]]},"Set date2":{"main":[[{"node":"Format date2","type":"main","index":0}]]},"Format date2":{"main":[[{"node":"Set commit date2","type":"main","index":0}]]},"Set commit date2":{"main":[[{"node":"List files from repository [GITHUB]2","type":"main","index":0}]]},"List files from repository [GITHUB]2":{"main":[[{"node":"Combine file names [GITHUB]2","type":"main","index":0}]]},"Combine file names [GITHUB]2":{"main":[[{